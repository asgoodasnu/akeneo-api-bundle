{
    "name": "asgoodasnu/akeneo-api-bundle",
    "description": "Akeneo API client",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Oliver Pintat",
            "email": "oliver.pintat@asgoodasnew.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "symfony/config": "^6.2.0",
        "symfony/dependency-injection": "^6.2.0",
        "symfony/http-kernel": "^6.2.0",
        "symfony/cache": "^6.2.0",
        "symfony/http-client": "^6.2.0"
    },
    "autoload": {
        "psr-4": {
            "Asgoodasnew\\AkeneoApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asgoodasnew\\AkeneoApiBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.2.0",
        "phpstan/phpstan": "~1.10.21",
        "friendsofphp/php-cs-fixer": "3.*"
    }
}
