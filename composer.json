{
    "name": "asgoodasnu/akeneo-api-bundle",
    "description": "Akeneo API client",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Oliver Pintat",
            "email": "oliver.pintat@asgoodasnew.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "symfony/config": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/cache": "^5.0",
        "symfony/http-client": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Asgoodasnew\\AkeneoApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asgoodasnew\\AkeneoApiBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^5.1",
        "phpstan/phpstan": "^1.8.11",
        "friendsofphp/php-cs-fixer": "^3.0"
    }
}
